    "husky": "^7.0.4",
    "jest-canvas-mock": "^2.3.1",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^12.1.7",
    "mrm": "2",
    "plop": "^3.0.5",
    "prettier": "^2.5.1",
    "webpack": "^5.66.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint",
      "prettier --check"
    ],
    "*.js": [
      "eslint",
      "prettier --check"
    ],
    "*.tsx": [
      "eslint",
      "prettier --check"
    ]
